	rem  * Method BTN_CONFIGS_ADD__ON_BUTTON_PUSH: 
	rem  * Event handler for the ON_BUTTON_PUSH event for BTN_CONFIGS_ADD
	rem  * @param BBjButtonPushEvent
	rem  */
	method public void BTN_CONFIGS_ADD__ON_BUTTON_PUSH(BBjButtonPushEvent pEvent!)
	    #C_Window101__ED_CFG_NAME!.setEnabled(1)
		#C_Window101__ED_CFG_NAME!.setText("")
		#C_Window101__ED_CFG_NAME!.focus()
		#C_Window101__ED_CFG_NAME!.showToolTipText()
		#C_Window101__LB_CONFIGS!.deselectAll()
		#C_Window101__LB_CFG_CPENTRIES!.removeAllItems()
		#C_ED_CLASSNAME!.setText("")
	methodend

