	method public void updateConfigArea()
        cfg$ = #C_Window101__LB_CONFIGS!.getSelectedItem()
        
        #C_Window101__ED_CFG_NAME!.setText(cfg$(6))
        #C_Window101__ED_CFG_NAME!.setEnabled(0)
        #C_Window101__LB_CFG_CPENTRIES!.removeAllItems()
        if cfg$>"" then
            #last_selected_model! = cfg$
            v! = new BBjVector()
            cp! = #model!.getCfgList().get(cfg$)
            it! = cp!.iterator()
            while it!.hasNext()
                cp_entry$ = it!.next()
                if cp_entry$ <> "(bbj_default)" then
                    v!.add(cp_entry$)
                fi
            wend
            #C_Window101__LB_CFG_CPENTRIES!.insertItems(0,v!)
            #C_ED_CLASSNAME!.setText(#model!.getClassname(cfg$))
        fi
     methodend
