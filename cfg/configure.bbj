	rem /**
	rem  * Method BTN_SAVE__ON_BUTTON_PUSH: 
	rem  * Event handler for the ON_BUTTON_PUSH event for BTN_SAVE
	rem  * @param BBjButtonPushEvent
	rem  */
	method public void BTN_SAVE__ON_BUTTON_PUSH(BBjButtonPushEvent pEvent!)
		   cfg$=cvs(#C_Window101__ED_CFG_NAME!.getText(),139)
		   if cfg$="" or cfg$(1,1)="(" then 
		        a=msgbox("Invalid Deployment Name!",0,"Error")
		        methodret
		   fi
		   
		   #last_selected_model!=cfg$
		   
		   
		   cpList! = #C_Window101__LB_CFG_CPENTRIES!.getAllItems()
		   #model!.writeCfg(cfg$,cpList!,#C_ED_CLASSNAME!.getText())
		   
		   #refreshScreen()
	methodend
	
	method private void refreshScreen()
	       #C_Window101__LB_CONFIGS!.removeAllItems()
	       
	       
	       it! = #model!.getCfgList().keySet().iterator()
	       v! = new BBjVector()
	       sel=-1
	       i=0
	       while it!.hasNext()
	            cfg$=it!.next()
	            if cfg$=#last_selected_model! then
	                sel=i
	            fi
	            v!.addItem(cfg$)
	            i=i+1
	       wend
	       
	       #C_Window101__LB_CONFIGS!.insertItems(0,v!)
	       
	       
	       if (v!.size()) then
                #C_Window101__LB_CONFIGS!.selectIndex(max(0,sel))
	            #updateConfigArea()
	       else
	            #C_Window101__ED_CFG_NAME!.setText("")
	            #C_Window101__LB_CFG_CPENTRIES!.removeAllItems()
	       fi
	       
	       #C_Window101__ED_CFG_NAME!.setEnabled(0)
	       
	methodend

